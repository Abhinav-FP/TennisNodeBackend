{
    "version": 2,
    "name": "Tennis Khelo Backend 2",
    "builds": [
      { 
        "src": "server.js", 
        "use": "@vercel/node",
        "config": {
          "maxDuration": 30,
          "memory": 3009
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  